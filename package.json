{
  "name": "lillypond-syntax-highlighter",
  "displayName": "Lillypond syntax highlighter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages"
