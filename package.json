{
  "name": "lillypond-syntax-highlighter",
  "displayName": "Lillypond syntax highlighter",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Programming Languages",
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "lillypond Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/lillypond Dark-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "lillypond",
        "aliases": [
          "lillypond",
          "lillypond"
        ],
        "extensions": [
          ".ly"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lillypond",
        "scopeName": "source.lillypond",
        "path": "./syntaxes/lillypond.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
